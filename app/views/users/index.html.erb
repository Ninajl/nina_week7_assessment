<h1>All Users</h1>

<%= link_to "Create User", new_user_path %>
<table>
  <thead>
    <tr>
      <th>User Name</th>
      <th>About</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.user_name, user_path(user) %></td>
      <td><%= user.about %></td>
    </tr>
  </tbody>
  <% end %>
</table>
